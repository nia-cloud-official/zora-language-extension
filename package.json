{
  "name": "zora-language",
  "displayName": "Zora Lumin Language Support",
  "description": "Syntax highlighting for Zora configuration files.",
  "version": "0.1.1",
  "publisher": "zora",
  "engines": {
    "vscode": "*"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "zora",
        "aliases": [
          "Zora",
          "zora"
        ],
        "extensions": [
          ".zora",
          ".zr"
        ],
        "filenames": [
          "*.zora",
          "*.zr"
        ]
      }
    ],
    "grammars": [
      {
        "language": "zora",
        "scopeName": "source.zora",
        "path": "./syntaxes/zora.tmLanguage.json"
      }
    ],
    "fileIcons": [
      {
        "fileMatch": [
          "*.zora",
          "*.zr"
        ],
        "icon": {
          "light": "icon.png",
          "dark": "icon.png"
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nia-cloud-official/zora-language-extension.git"
  },
  "bugs": {
    "url": "https://github.com/nia-cloud-official/zora-language-extension/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "vscode": ""
  },
  "icon": "icon.png"
}